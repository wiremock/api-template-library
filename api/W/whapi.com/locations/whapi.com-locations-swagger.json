                "schema" : {
                  "$ref" : "#/components/schemas/locationsErrors"
                },
                "example" : "{\n    \"errors\" : \n    [\n        {\n          \"code\":\"20303\",\n          \"message\":\"Country not Found\"\n        }\n    ]\n}\n"
              }
            }
          }
        }
      }
    },
